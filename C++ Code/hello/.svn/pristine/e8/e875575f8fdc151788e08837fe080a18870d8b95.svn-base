#include <iostream>
#include<bits/stdc++.h>




using namespace std;

int main()
{
    string name;
    int i =0;
    int len;

    getline(cin, name);
    if(name == "") {
        cout << "Hello, CP2 " << endl;
    }
    else {

        while (getline(cin, name)) {
            len = name.length();

            for (i =0; i < len; i++) {
                name[i] = tolower(name[i]);
            }
            name[0] = toupper(name[0]);
            cout << "Hello, " << name << endl;
            getline(cin, name);






        }
    }

    return 0;




}
