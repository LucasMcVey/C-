#include <iostream>
#include<bits/stdc++.h>
#include <string>
#include <cctype>


using namespace std;

int main()
{
    string name;

    cout <<  "";

    while(getline(cin, name)) {
        if(name == "") {
            cout << "Hello, CP2 " << endl;
            return 0;
        }
        else
            transform(name.begin(), name.end(), name.begin(), ::tolower);
           name[0] = toupper(name[0]);
           cout << "Hello, " << name << endl;


    }



}
